<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
 <context-param>
   <param-name>driver</param-name>
   <param-value>com.mysql.jdbc.Driver</param-value>
 </context-param>
 <context-param>
   <param-name>url</param-name>
   <param-value>jdbc:mysql://localhost/calculator</param-value>
 </context-param>
 <context-param>
    <param-name>user</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <param-name>pwd</param-name>
    <param-value>12345</param-value>
  </context-param>
   <servlet>
    <servlet-name>s1</servlet-name>
    <servlet-class>com.knowuhub.theCalculator.Thinking</servlet-class>
  </servlet>
  <servlet>
   <servlet-name>s2</servlet-name>
   <servlet-class>com.knowuhub.theCalculator.MentalAge</servlet-class>
  </servlet>
  <servlet>
   <servlet-name>s3</servlet-name>
   <servlet-class>com.knowuhub.theCalculator.Marry</servlet-class>
  </servlet>
  <servlet>
   <servlet-name>s4</servlet-name>
   <servlet-class>com.knowuhub.theCalculator.TrueorFalse</servlet-class>
  </servlet>
   <servlet>
   <servlet-name>s5</servlet-name>
   <servlet-class>com.knowuhub.theCalculator.IQtest</servlet-class>
  </servlet>
 <servlet-mapping>
    <servlet-name>s1</servlet-name>
    <url-pattern>/thinking</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>s2</servlet-name>
    <url-pattern>/mentalAge</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>s3</servlet-name>
    <url-pattern>/marry</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>s4</servlet-name>
    <url-pattern>/truefalse</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>s5</servlet-name>
    <url-pattern>/iqtest</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>home.html</welcome-file>
   </welcome-file-list>
</web-app>